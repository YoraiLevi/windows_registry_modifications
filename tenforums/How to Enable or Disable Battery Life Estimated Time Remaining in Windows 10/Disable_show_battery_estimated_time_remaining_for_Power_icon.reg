[HKEY_LOCAL_MACHINE\SYSTEM\CurrentControlSet\Control\Power]
"EnergyEstimationDisabled"=dword:00000001
"UserBatteryDischargeEstimator"=dword:00000001
"EnergyEstimationEnabled"=-
